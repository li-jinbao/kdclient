    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Run Zibo",
            "type": "debugpy",
            "request": "launch",
            "envFile": "${workspaceFolder}/dev.env",
            "program": "${workspaceFolder}/src/zibo.py",
            "args": []
        },        
        {
            "name": "Run London",
            "type": "debugpy",
            "request": "launch",
            "envFile": "${workspaceFolder}/london.env",
            "program": "${workspaceFolder}/src/london_simple.py",
            "args": []
        },        
        {
            "name": "Run SG Pickdrop",
            "type": "debugpy",
            "request": "launch",
            "envFile": "${workspaceFolder}/sg.env",
            "program": "${workspaceFolder}/src/sg_pickdrop.py",
            "args": []
        },        
    ]
}